<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="230.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/null"
            fx:controller="tsk.jgnk.watchdist.controller.ChangePasswordController">
    <children>
        <Label fx:id="infoHeader" layoutX="14.0" layoutY="14.0" prefHeight="30.0" prefWidth="272.0" text="-"
               wrapText="true" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
        <PasswordField fx:id="currentPassword" layoutX="14.0" layoutY="55.0" promptText="%current.password"
                       AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"/>
        <PasswordField fx:id="newPassword1" layoutX="14.0" layoutY="90.0" promptText="%new.password"
                       AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"/>
        <PasswordField fx:id="newPassword2" layoutX="14.0" layoutY="124.0" promptText="%new.password.again"
                       AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0"/>
        <Button defaultButton="true" layoutX="14.0" layoutY="158.0" mnemonicParsing="false" onAction="#changePassword"
                prefHeight="30.0" prefWidth="200.0" text="%change.password" AnchorPane.leftAnchor="14.0"
                AnchorPane.rightAnchor="14.0">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Button>
        <Label fx:id="errorLabel" layoutX="14.0" layoutY="258.0" prefHeight="23.0" prefWidth="144.0" text="-"
               textFill="RED" visible="false" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0"
               AnchorPane.rightAnchor="14.0">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
